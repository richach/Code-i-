/WAP to print all non-zero number at beginning and zeros at the end

#include<iostream>
#define MAX_SIZE 10
using namespace std;
int main()
{
		 int size=8;
	int arr[]={0,11,0,2,0,10,0,1};
	 int arr1[size];
	 int counter=0;

for(int i=0; i<size; i++)
{
	if(arr[i]!=0)
	{
		arr1[counter]=arr[i];
		counter++;
	}

}

for(int i=counter; i<size;i++)
{
	arr1[i]=0;
}

		for(int i=0; i<size; i++)
		{
			cout<<arr1[i]<<" ";
		}
}

output: 11,2,10,1,0,0,0,0
