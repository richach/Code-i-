//Take 10 numbers as the input and print sum of those numbers, ignore all negative numbers.


#include<iostream>
#define MAX_SIZE 50
using namespace std;
int main()
{
	int arr[MAX_SIZE];
	int size;
    	cin>>size;
	
	for(int i=0; i<=size-1; i++)
	{
	   cin>>arr[i];	
	}
	
	int sum=0;
	
	for(int i=0;i<=size-1;i++)
	{
		if(arr[i]>0)
		{
			sum=sum+arr[i];
		}
	}
	cout<<sum;
	
	
}
