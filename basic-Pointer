#include<iostream>
using namespace std;
int main()
{
	int var=9,*p;
	
	p=&var; // assigning the address of he varibale var in ponter p, so that it wll help n execution of  (L1)
	
	cout<<&var <<endl; //print the address of variable
	
	cout<<p <<endl; //print the address of variable
	
	cout<<*p; //print the value of varbale (L1)
	
}
