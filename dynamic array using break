//Take 10 numbers in input(user input) and print sum of those numbers, break execution if the number is negative. 

#include<iostream>
#define MAX_SIZE 50
using namespace std;
int main()
{
	int arr[MAX_SIZE];
	int size;
    	cin>>size;
	
	for(int i=0; i<=size-1; i++)
	{
	   cin>>arr[i];	
	}
	
	int sum=0;
	
	for(int i=0;i<=size-1;i++)
	{
		if(arr[i]<0)
		{
			break;
		}
		
		else
		{
			sum=sum+arr[i];
		}
	}
	cout<<sum;
	
	
}
