// Take 10 numbers in input and print sum of those numbers, break execution if the number is negative. 

#include<iostream>
using namespace std;
int main()
{
	int arr[10]={2,4,7,5,-3,4,-2,4,3,2};
	int sum=0;
	
	for(int i=0;i<=9;i++)
	{
	   if(arr[i]<0)
	   {
	   	break;
	   }
	   else
	   {
	   	sum=sum+arr[i];
	   }
		
	}
	cout<<sum;
	
}
